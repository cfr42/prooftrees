\documentclass{article}
\usepackage[tableau]{prooftrees}
\usepackage{turnstile}
\newcommand*{\tnot}{\mathord{\sim}}
\newcommand*{\horseshoe}{\mathbin{\supset}}
\newcommand*{\liff}{\mathbin{\leftrightarrow}}
\newbox\tableaubox
\forestset{draw tree box=\tableaubox}
\begin{document}
\START
% ateb David Carlisle: https://tex.stackexchange.com/a/674846/ (cwestiwn gusbrs)
\TEST{prooftrees-t83-wffs-from-proof-line-no-to}{%
  \begin{tableau}{%
      to prove={\{P \vee (Q \vee \tnot R), P \horseshoe \tnot R, Q \horseshoe \tnot R\} \sststile{}{} \tnot R},
      nodewalk node={draw=magenta,rounded corners},
      nodewalk to node={a}{wffs from proof line no to={6}{7}},
      before annotating={for nodewalk={wffs from proof line no to={3}{5}}{blue,typeset node}},
    }
    [P \vee (Q \vee \tnot R)
      [P \horseshoe \tnot R, nodewalk node={draw=orange},nodewalk to node={b}{wffs from proof line no to={2}{4}},
        [Q \horseshoe \tnot R
          [\tnot\tnot R
            [P
              [\tnot P, close={:!u,!c}
              ]
              [\tnot R,  close={:!uu,!c}
              ]
            ]
            [Q \vee \tnot R
              [Q
                [\tnot Q, close={:!u,!c}
                ]
                [\tnot R,  close={:!uuu,!c}
                ]
              ]
              [\tnot R, close={:!uu,!c}
              ]
            ]
          ]
        ]
      ]
    ]
  \end{tableau}
  \showbox\tableaubox
}%
